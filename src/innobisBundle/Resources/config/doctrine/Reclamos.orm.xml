    <field name="departamento" type="integer" column="departamento" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="torre" type="string" column="torre" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="categoria" type="string" column="categoria" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="gravedad" type="integer" column="gravedad" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fechaReclamo" type="date" column="fechaReclamo" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fechaSolucion" type="date" column="fechaSolucion" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="detalle" type="string" column="detalle" length="80" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="observacion" type="text" column="observacion" length="65536" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="recinto" type="text" column="recinto" length="255" nullable="false">
      <options>
        <option name="fixed"/>
